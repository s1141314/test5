<html>
    <head> 
        <title>Google Map Example</title>
		<link rel="stylesheet" type="text/css" href="layout.css">
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true&key=AIzaSyA0Z5LJKSBIvRX9mdnowyHqaak930az53M"></script>
        <script type="text/javascript">
            var loadMap = function() 
            {
				
				var myLatLng = {lat: <%=lat%>, lng: <%=lon%>};
                var map = new google.maps.Map(document.getElementById("map"),{
				      zoom: 20,
				      center: myLatLng
				}); 
				var map = new google.maps.Marker({
					position: myLatLng,
					map: map,
					title: <%=title%>
				});
            };
            window.onload= loadMap;
        </script>
    </head>
    <body>
	 	<header><h1><%=title%></h1></header>
			<aside>
        	<div id="map" style="width:500px;height:500px;">
			</aside>
<button class="w3-btn w3-section w3-teal w3-ripple" onclick="goBack()">Back</button>
    </body>
</html>
